config_version=5

[application]

config/name="YADR"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")

[autoload]

Global="*res://scripts/managers/Global.cs"
SteamManager="*res://scripts/managers/SteamManager.cs"

[display]

